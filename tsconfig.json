{
  "extends": "astro/tsconfigs/base",
  "include": [".astro/types.d.ts", "./src/**/*.{astro,html,js,jsx,ts,tsx}"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@src/*": ["./src/*"],
      "@layouts/*": ["src/layouts/*"],
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"],
      "@types/*": ["src/types/*"],
      "@pages/*": ["src/pages/*"],
      "@menu/*": ["src/components/menu/*"]
    }
  }
}
